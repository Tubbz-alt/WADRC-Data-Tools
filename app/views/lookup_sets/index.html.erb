<% if current_user.role == 'Admin_High'%>
<h1>Listing lookup_sets</h1>

<table>
  <tr>
    <th>Description</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @lookup_sets.each do |lookup_set| %>
  <tr>
    <td><%= lookup_set.description %></td>
    <td><%= link_to 'Show', lookup_set %></td>
    <td><%= link_to 'Edit', edit_lookup_set_path(lookup_set) %></td>
    <td><%= link_to 'Destroy', lookup_set, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Lookup set', new_lookup_set_path %>
<% else %>
  Your <%= current_user.role %> role does not allow access. 
<% end %>