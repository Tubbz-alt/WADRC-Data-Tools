<p id="notice"><%= notice %></p>

<h1>Folderpremissions</h1>

<table>
  <thead>
    <tr>
      <th>Actual vs planned</th>
      <th>Network user</th>
      <th>Network group</th>
      <th>Permission read</th>
      <th>Permission write</th>
      <th>Permission execute</th>
      <th>Folder</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @folderpremissions.each do |folderpremission| %>
      <tr>
        <td><%= folderpremission.actual_vs_planned %></td>
        <td><%= folderpremission.network_user %></td>
        <td><%= folderpremission.network_group %></td>
        <td><%= folderpremission.permission_read %></td>
        <td><%= folderpremission.permission_write %></td>
        <td><%= folderpremission.permission_execute %></td>
        <td><%= folderpremission.folder_id %></td>
        <td><%= link_to 'Show', folderpremission %></td>
        <td><%= link_to 'Edit', edit_folderpremission_path(folderpremission) %></td>
        <td><%= link_to 'Destroy', folderpremission, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Folderpremission', new_folderpremission_path %>
