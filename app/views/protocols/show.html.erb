<p id="notice"><%= notice %></p>
<% if current_user.role == 'Admin_High'%>
<p>
  <b>Name:</b>
  <%= @protocol.name %>
</p>

<p>
  <b>Abbr:</b>
  <%= @protocol.abbr %>
</p>

<p>
  <b>Path:</b>
  <%= @protocol.path %>
</p>

<p>
  <b>Description:</b>
  <%= @protocol.description %>
</p>


<%= link_to 'Edit', edit_protocol_path(@protocol) %> |
<%= link_to 'Back', protocols_path %>

<% else %>
  Your <%= current_user.role %> role does not allow access. 
<% end %>
