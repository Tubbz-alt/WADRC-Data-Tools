<% if current_user.role == 'Admin_High' or current_user.role == 'Admin_Low'%>
<h1>Listing lumbarpuncture_results</h1>

<table>
  <tr>
    <th>Lumbarpuncture</th>
    <th>Lookup lumbarpuncture</th>
    <th>Value</th>
    <th>Value string</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @lumbarpuncture_results.each do |lumbarpuncture_result| %>
  <tr>
    <td><%= lumbarpuncture_result.lumbarpuncture_id %></td>
    <td><%= lumbarpuncture_result.lookup_lumbarpuncture_id %></td>
