<% if current_user.role == 'Admin_High' or current_user.role == 'Admin_Medium'%>
<h1>Listing neuropsyches</h1>

<table>
  <tr>
    <th>Appointment</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @neuropsyches.each do |neuropsych| %>
  <tr>
    <td><%= neuropsych.appointment_id %></td>
    <td><%= link_to 'Show', neuropsych %></td>
    <td><%= link_to 'Edit', edit_neuropsych_path(neuropsych) %></td>
    <td><%= link_to 'Destroy', neuropsych, :method => :delete,data: {confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Neuropsych', new_neuropsych_path %>
<% end %>
