<p id="notice"><%= notice %></p>

<p>
  <b>Description:</b>
  <%= @cg_table_type.description %>
</p>

<p>
  <b>Table type:</b>
  <%= @cg_table_type.table_type %>
</p>

<p>
  <b>Protocol:</b>
<% if !(@cg_table_type.protocol_id).nil? and !(@cg_table_type.protocol_id).to_s.empty?%>
<%= (Protocol.find(@cg_table_type.protocol_id)).name %>
<% end %>
</p>
<p>
  <b>Active status:</b>
  <%= @cg_table_type.status_flag %>
</p>
  <b>Hide dates in all queries:</b>
  <%= @cg_table_type.hide_date_flag %>
</p>
  <b>default_open_flag:</b>
  <%= @cg_table_type.default_open_flag %>
</p>
  <b>display_order:</b>
  <%= @cg_table_type.display_order %>
</p>
  <b>Hdisplay_name:</b>
  <%= @cg_table_type.display_name %>
</p>
  <b>editable_dashboard_table_type_flag:</b>
  <%= @cg_table_type.editable_dashboard_table_type_flag %>
</p>
<% if current_user.role == 'Admin_High' or current_user.role == 'Admin_Medium'%>
<%= link_to 'Edit', edit_cg_table_type_path(@cg_table_type) %> |
<% end %>
<%= link_to 'Back', cg_table_types_path %>
