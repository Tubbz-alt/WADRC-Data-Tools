<h1>New neuropsych_assessment</h1>

<%= form_for(@neuropsych_assessment) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :score %><br />
    <%= f.text_field :score %>
  </p>
  <p>
    <%= f.label :score_type %><br />
    <%= f.text_field :score_type %>
  </p>
  <p>
    <%= f.label :test_name %><br />
    <%= f.text_field :test_name %>
  </p>
  <p>
    <%= f.label :note %><br />
    <%= f.text_area :note %>
  </p>
  <p>
    <%= f.submit "Create" %>
  </p>
<% end %>

<%= link_to 'Back', neuropsych_assessments_path %>
